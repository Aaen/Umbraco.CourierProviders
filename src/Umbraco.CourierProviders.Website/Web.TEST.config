<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  
  
<system.web>
        <compilation xdt:Transform="RemoveAttributes(debug)" />
        <caching>
            <outputCache enableOutputCache="true" xdt:Transform="SetAttributes" />
        </caching>
    </system.web><system.webServer xdt:Transform="InsertIfMissing">
        <staticContent xdt:Transform="InsertIfMissing">
            <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365.0:00:00" xdt:Transform="InsertIfMissing" />
        </staticContent>
    </system.webServer></configuration>